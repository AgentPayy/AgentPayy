{
  "name": "@agentpayy/sdk",
  "version": "1.0.1",
  "description": "AgentPayyKit TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "payments",
    "ethereum",
    "web3",
    "wallet"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentPayy/AgentPayy.git",
    "directory": "sdk/typescript"
  },
  "homepage": "https://github.com/AgentPayy/AgentPayy#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ethers": "^6.8.0",
    "axios": "^1.6.0",
    "viem": "^2.21.0",
    "wagmi": "^2.12.0",
    "permissionless": "^0.2.0",
    "@biconomy/account": "^4.0.0",
    "@zerodev/sdk": "^5.0.0",
    "@zerodev/ecdsa-validator": "^5.0.0",
    "@alchemy/aa-core": "^3.0.0",
    "@walletconnect/universal-provider": "^2.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
